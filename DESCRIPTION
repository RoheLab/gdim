Package: gdim
Title: Graph Dimensionality
Version: 0.0.0.9000
Authors@R: c(
    person("Fan", "Chen", , "fchen365@gmail.com", role = "aut",
           comment = c(ORCID = "0000-0003-4508-6023")),
    person("Alex", "Hayes", , "alexpghayes@gmail.com", role = c("cre", "aut"),
